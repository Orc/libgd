	gd_io.o \
	gd_io_dp.o \
	gd_io_file.o \
	gd_io_ss.o \
	gd_jpeg.o \
	gd_matrix.o \
	gd_nnquant.o \
	gd_png.o \
	gd_rotate.o \
	gd_security.o \
	gd_ss.o \
	gd_tga.o \
	gd_tiff.o \
	gd_topal.o \
	gd_transform.o \
	gd_version.o \
	gd_wbmp.o \
	gd_webp.o \
	gd_xbm.o \
	gdcache.o \
	gdfontg.o \
	gdfontl.o \
	gdfontmb.o \
	gdfonts.o \
	gdfontt.o \
	gdft.o \
	gdfx.o \
	gdhelpers.o \
	gdkanji.o \
	gdtables.o \
	gdxpm.o \
	wbmp.o


$(LIBRARY): $(OBJS) VERSION
	./librarian.sh make $(LIBRARY) VERSION $(OBJS) 

VERSION: mkversion
	./mkversion > VERSION

mkversion: mkversion.c

# since I hand-generate the version from gd.h,
# and since the librarian wants a version file,
# that means I may need to regenerate it so I
# can delete the library.
clean-library: VERSION
	rm -f `./librarian.sh files $(LIBRARY) VERSION`

clean: clean-library
	rm -f $(OBJS) mkversion VERSION

distclean spotless: clean
	@DISTCLEAN@ @GENERATED_FILES@ @CONFIGURE_FILES@

install: $(LIBRARY)
	@INSTALL@ -c gd.h gd_io.h gdfx.h $(INCDIR)
	./librarian.sh install $(LIBRARY) VERSION $(LIBDIR)



